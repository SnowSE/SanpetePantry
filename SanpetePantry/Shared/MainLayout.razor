@inherits LayoutComponentBase

<PageTitle>Sanpete Pantry</PageTitle>

<div class="page">
    <div class="sidebar">
        <NavMenu />
    </div>

    <div class="main">
        <div class="top-row px-4 auth">
            <div class="d-flex justify-content-start align-items-center">
                <span>Follow Us:</span>
